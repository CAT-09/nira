autoscaling <- function (df) {
if (! is.data.frame (df)) {stop ("deve ser um data.frame")}
for (i in 1:NCOL(df)){
if (is.numeric(df[,i])==TRUE){
df[,i]<- df[,i]/sqrt(sum(df[,i]^2)/(length(df[,i])-1))
}else{
df[,i]=df[,i]}}
return (df) }
center <- function (df) {
if (! is.data.frame (df)) {stop ("deve ser um data.frame")}
for (i in 1:NCOL(df)){
if (is.numeric(df[,i])==TRUE){
df[,i] = df[,i]-mean(df[,i])
}else{
df[,i]=df[,i]}}
return (df) }
Center_scale_df <- function (df) {
if (! is.data.frame (df)) {stop ("deve ser um data.frame")}
for (i in 1:NCOL(df)){
if (is.numeric(df[,i])==TRUE){
df[,i] = (df[,i]-mean(df[,i]))/sd(df[,i])
}else{
df[,i]=df[,i]}}
return (df) }
